	IUT_IP = "IUT_IP"
	TD_IP  = "TD_IP"
  
	DEVICE_ID = Device, 47814          -- Device object instance for IUT.
	DEVNUM = 47813                      -- The number of the device
	TD_PORT = "bip"
	
	OBJECT_IDENTIFIER = Device, 47814 

  	LOW_LIMIT = 40000		; Low limit of a device range that will include the IUT.
  	HIGH_LIMIT = 50000   	; High limit of a device range that will include the IUT.

  	OUTSIDE_LOW = 1990		; Low limit of a device range that won't include the IUT.
  	OUTSIDE_HIGH = 2000		; High limit of a device range that won't include the IUT.

	OBJECT_NAME = ANSI,"BAC_47813"   ; This value must be changed

